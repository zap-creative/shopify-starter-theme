{
  "name": "shopify-starter-theme",
  "version": "0.0.1",
  "description": "A Shopify starter theme using Theme Kit and Webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "deploy": "NODE_ENV=production webpack -p && theme deploy --dir=theme",
    "watch": "concurrently -n w: npm:watch:*",
    "watch:assets": "webpack --watch --color --progress",
    "watch:theme": "theme watch --notify=theme-ready.tmp --dir=theme --allow-live",
    "prod:build": "NODE_ENV=production webpack --env=analyze -p",
    "prod:deploy": "NODE_ENV=production webpack -p && theme deploy --env=production --dir=theme"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/runtime": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.2",
    "core-js": "^3.10.1",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.11",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^8.2.10",
    "postcss-loader": "^4.2.0",
    "read-yaml": "^1.1.0",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "style-loader": "^1.3.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "svelte": "^3.37.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.7.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@shopify/theme-a11y": "^3.1.0",
    "@shopify/theme-addresses": "^4.1.1",
    "@shopify/theme-cart": "^3.1.0",
    "@shopify/theme-currency": "^3.1.0",
    "@shopify/theme-images": "^3.1.0",
    "@shopify/theme-predictive-search": "^4.1.1",
    "@shopify/theme-product": "^4.1.1",
    "@shopify/theme-product-form": "^4.1.1",
    "@shopify/theme-sections": "^4.1.1",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.3.1",
    "lazysizes": "^5.3.2",
    "lodash-es": "^4.17.21",
    "tailwindcss": "^2.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
