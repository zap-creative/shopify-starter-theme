{
  "name": "shopify-starter-theme",
  "version": "0.0.1",
  "description": "A Shopify starter theme using Theme Kit and Webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:prod": "npm run prod:env && npm run prod:build",
    "watch": "concurrently -n w: npm:watch:*",
    "watch:assets": "webpack --watch --color",
    "watch:theme": "theme watch --notify=theme-ready.tmp --dir=theme --allow-live",
    "prod:env": "env NODE_ENV=production",
    "prod:build": "webpack --mode=production --env=analyze",
    "prod:deploy": "theme deploy --env=production --dir=theme --allow-live",
    "deploy": "theme deploy --env=production --dir=theme --allow-live"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.0.2",
    "core-js": "^3.10.1",
    "css-loader": "^5.2.1",
    "cssnano": "^5.0.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^3.1.2",
    "mini-css-extract-plugin": "^1.4.1",
    "postcss": "^8.2.10",
    "postcss-load-config": "^3.0.1",
    "postcss-loader": "^5.2.0",
    "read-yaml": "^1.1.0",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "svelte": "^3.37.0",
    "svelte-loader": "^3.1.1",
    "svelte-preprocess": "^4.7.0",
    "svg-sprite-loader": "^6.0.5",
    "webpack": "^5.30.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@shopify/theme-a11y": "^4.1.1",
    "@shopify/theme-addresses": "^4.1.1",
    "@shopify/theme-cart": "^4.1.1",
    "@shopify/theme-currency": "^4.1.1",
    "@shopify/theme-images": "^4.1.1",
    "@shopify/theme-predictive-search": "^4.1.1",
    "@shopify/theme-product": "^4.1.1",
    "@shopify/theme-product-form": "^4.1.1",
    "@shopify/theme-sections": "^4.1.1",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "lazysizes": "^5.3.2",
    "lodash-es": "^4.17.21",
    "micromodal": "^0.4.6",
    "tailwindcss": "^2.1.1"
  }
}
